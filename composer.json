{
  "name": "psilocyberunner/acoustid",
  "description": "PHP wrapper for audio fingerprint search service at acoustid.org",
  "type": "library",
  "keywords": [
    "acoustid",
    "library",
    "php",
    "api"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Petrov",
      "email": "petrovgm@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "ext-curl": "*",
    "php": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "phpstan/phpstan": "^0.11.12",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/var-dumper": "*",
    "filp/whoops": "^2.4",
    "james-heinrich/getid3": "^1.9"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "AcoustId\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "component-dir": "public/packages",
    "vendor-dir": "vendor"
  },
  "scripts": {
    "phpstan": "./vendor/bin/phpstan analyse src --level max",
    "phpunit": "./vendor/bin/phpunit  -c ./phpunit.xml"
  }
}
